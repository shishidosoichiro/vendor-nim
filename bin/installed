#!/usr/bin/env sh

# Exit, if return non-zero or use an undefined variable.
set -eu

# Restore the current directory, if exit.
pwd="$PWD"
trap "cd \"$pwd\"" EXIT

# cd workdir
scriptfile="$(realpath "$0")"
bin="$(dirname "$scriptfile")"
workdir="$(dirname "$bin")"
cd "$workdir"

# Start
cd versions
ls -1
cd ..
